# Deploy script (Deploy.s.sol)
PRIVATE_KEY=your_private_key_here
AGENT_ADDRESS=0x...
HUMAN_ADDRESS=0x...
USDC_ADDRESS=0x036CbD53842c5426634e7929541eC2318f3dCF7e
MAX_AMOUNT=1000000000000000000
DAILY_CAP=10000000000000000000
APPROVAL_THRESHOLD=500000000000000000

# Dashboard reads this file; set these so the app uses the same config
# WalletConnect (get free ID: https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id
# Network (or set RPC_URL/CHAIN_ID â€” dashboard maps them to NEXT_PUBLIC_*)
# For Base Sepolia (default):
NEXT_PUBLIC_CHAIN_ID=84532
NEXT_PUBLIC_RPC_URL=https://sepolia.base.org
# For local Anvil, use instead:
# NEXT_PUBLIC_CHAIN_ID=31337
# NEXT_PUBLIC_RPC_URL=http://127.0.0.1:8545
# Optional: default wallet list (comma-separated). If unset, AGENT_ADDRESS is used.
# NEXT_PUBLIC_AGENT_WALLET_ADDRESSES=0x...,0x...

# Factory for creating wallets from the app (deploy with: forge script script/DeployFactory.s.sol --broadcast)
NEXT_PUBLIC_FACTORY_ADDRESS=
# Or: FACTORY_ADDRESS=0x...
